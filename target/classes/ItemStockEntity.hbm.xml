<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.emercial.entities.ItemStockEntity" table="item_stock" schema="ecommercial">
        <id name="itemId">
            <column name="item_id" sql-type="int(11)"/>
        </id>
        <property name="stock">
            <column name="stock" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>